<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.okfruitapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- Agrega la actividad InicioSesionActivity aquí -->
        <activity
            android:name=".InicioSesionActivity"
            android:label="Inicio de Sesión" >
        </activity>

        <activity
            android:name=".MuestraActivity"
            android:label="Muestra" >
        </activity>

        <activity
            android:name=".SubirImagenActivity"
            android:label="Subir imagen" >
        </activity>

        <activity
            android:name=".ResultadoAnalisisActivity"
            android:label="Resultados del analisis "
            tools:ignore="Instantiatable,MissingClass">
        </activity>



    </application>

</manifest>